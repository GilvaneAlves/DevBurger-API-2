{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["**/*.json"],
      "plugins": ["json"],
      "extends": ["plugin:json/recommended"]
    }
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
